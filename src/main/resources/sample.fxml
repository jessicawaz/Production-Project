                                        <Insets/>
                                    </GridPane.margin>
                                </Label>
                                <TextField fx:id="txtManufacturer" alignment="CENTER" maxHeight="-Infinity"
                                           maxWidth="-Infinity" prefHeight="25.0" prefWidth="219.0"
                                           GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1"
                                           GridPane.valignment="CENTER"/>
                                <Label alignment="CENTER" layoutX="160.0" layoutY="119.0" prefHeight="27.0"
                                       text="Item Type" GridPane.halignment="CENTER" GridPane.rowIndex="2"
